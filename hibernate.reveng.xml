<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table name="user" catalog="cqzrj" class="User">
		<primary-key>
			<generator class="assigned"></generator>
		</primary-key>
	</table>
	<table name="users" catalog="cqzrj" class="Users">
		<primary-key>
			<generator class="identity"></generator>
		</primary-key>
	</table>
	<table name="tags" catalog="cqzrj" class="Tags">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="messages" catalog="cqzrj" class="Messages">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="movies" catalog="cqzrj" class="Movies">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="moviestag" catalog="cqzrj" class="Moviestag">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="sites" catalog="cqzrj" class="Sites">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="lives" catalog="cqzrj" class="Lives">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
<table name="trailers" catalog="cqzrj" class="Trailers">
	<primary-key>
		<generator class="increment"></generator>
	</primary-key>
</table>
</hibernate-reverse-engineering>